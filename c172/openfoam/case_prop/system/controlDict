FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       pimpleFoam;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           0.01;

deltaT            1e-5;
writeControl      adjustableRunTime;
writeInterval     0.01;

purgeWrite        0;

writeFormat       binary;

writePrecision    6;

writeCompression  off;

timeFormat        general;

timePrecision     6;

runTimeModifiable true;

adjustTimeStep    yes;

maxCo             2;

functions
{
    #include "forceCoeffs"
}

